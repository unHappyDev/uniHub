{
  "name": "auth-sample",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run services:seed && npm run next && npm run services:stop",
    "services:seed": "kill-port 3000 && npm run services:up && npm run migration:run && npm run migration:seed",
    "services:up": "npm run docker:compose -- up -d",
    "services:stop": "npm run docker:compose -- stop",
    "services:down": "npm run docker:compose -- down",
    "docker:compose": "docker compose --env-file .env -f docker-compose.yml",
    "migration:create": "npx prisma migrate dev --name init --create-only",
    "migration:run": "tsx -r dotenv-expand/config infra/scripts/wait-for-db-connection-ready.ts",
    "migration:dry-run": "npx prisma migrate deploy --preview-feature",
    "migration:seed": "tsx infra/scripts/seed-database.ts",
    "preconcurrently": "kill-port 3000 && npm run services:up",
    "concurrently": "concurrently -s first -P -k -n next,vitest \"npm run next\" \"vitest {@}\"",
    "postconcurrently": "npm run services:stop",
    "test": "npm run concurrently -- --hide next -- run",
    "test:watch": "vitest watch",
    "test:watch:services": "npm run concurrently -- -- watch",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint . --max-warnings 0",
    "lint:prettier": "prettier --check .",
    "lint:fix": "eslint --fix . && prettier --write .",
    "commit": "cz",
    "next": "next dev --turbopack",
    "start": "next start",
    "postinstall": "prisma generate",
    "build": "prisma migrate deploy && next build",
    "prepare": "husky",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.1.1",
    "@prisma/client": "^6.9.0",
    "@prisma/extension-accelerate": "^2.0.1",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@react-email/render": "^1.1.2",
    "@tanstack/react-query": "^5.80.6",
    "async-retry": "^1.3.3",
    "axios": "^1.10.0",
    "bcrypt": "^6.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cookie": "^1.0.2",
    "cron": "^4.3.1",
    "dotenv-expand": "^12.0.2",
    "framer-motion": "^12.23.6",
    "is-in-subnet": "^4.0.1",
    "kill-port": "^2.0.1",
    "lucide-react": "^0.513.0",
    "next": "15.3.3",
    "next-themes": "^0.4.6",
    "nodemailer": "^7.0.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.60.0",
    "tailwind-merge": "^3.3.1",
    "uuid": "^11.1.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@react-email/components": "^0.0.41",
    "@tailwindcss/postcss": "^4",
    "@types/async-retry": "^1.4.9",
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^20.19.5",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/supertest": "^6.0.3",
    "concurrently": "^9.2.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.6.1",
    "eslint": "^9.30.1",
    "eslint-config-next": "^15.3.5",
    "husky": "^9.1.7",
    "prisma": "^6.9.0",
    "set-cookie-parser": "^2.7.1",
    "supertest": "^7.1.1",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.4",
    "tw-animate-css": "^1.3.5",
    "typescript": "^5.8.3",
    "vitest": "^3.2.3"
  }
}
